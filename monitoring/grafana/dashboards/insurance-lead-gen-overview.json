{
  "dashboard": {
    "title": "Insurance Lead Gen - System Overview",
    "uid": "insurance-lead-gen-overview",
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Status",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 4},
        "targets": [
          {
            "expr": "up{job=~\"api-service|data-service|orchestrator|backend-python\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"value": 0, "text": "DOWN", "color": "red"},
              {"value": 1, "text": "UP", "color": "green"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "API Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{service}} - {{method}} {{path}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "API Response Time (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "{{service}} - {{status}}"
          }
        ]
      },
      {
        "id": 5,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "{{instance}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Usage %"
          }
        ]
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "pg_stat_activity_count",
            "legendFormat": "{{datname}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "Redis Keys",
        "type": "stat",
        "gridPos": {"x": 0, "y": 28, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "redis_db_keys"
          }
        ]
      },
      {
        "id": 9,
        "title": "Lead Processing Queue Depth",
        "type": "graph",
        "gridPos": {"x": 6, "y": 28, "w": 18, "h": 8},
        "targets": [
          {
            "expr": "queue_depth",
            "legendFormat": "{{queue}}"
          }
        ]
      },
      {
        "id": 10,
        "title": "AI Model Latency",
        "type": "graph",
        "gridPos": {"x": 0, "y": 36, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ai_model_latency_seconds_bucket[5m]))",
            "legendFormat": "{{model}}"
          }
        ]
      },
      {
        "id": 11,
        "title": "Active Alerts",
        "type": "alertlist",
        "gridPos": {"x": 12, "y": 36, "w": 12, "h": 8},
        "options": {
          "showOptions": "current",
          "maxItems": 20,
          "sortOrder": 1
        }
      }
    ]
  }
}
