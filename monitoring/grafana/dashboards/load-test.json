  "panels": [
    {
      "title": "Request Rate (RPS)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count[1m]))",
          "legendFormat": "Total RPS"
        }
      ]
    },
    {
      "title": "p99 Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{status=~\"5..\"}[1m])) / sum(rate(http_request_duration_seconds_count[1m]))",
          "legendFormat": "Error Rate"
        }
      ]
    },
    {
      "title": "Pod Replicas",
      "type": "timeseries",
      "targets": [
        {
          "expr": "kube_deployment_status_replicas_available{deployment=~\"api|data-service|orchestrator\"}",
          "legendFormat": "{{deployment}}"
        }
      ]
    }
  ],
